  "license": "Apache-2.0",
  "devDependencies": {
    "@types/ethereum-protocol": "^1.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.4.1",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.13.0",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "prettier": "catalog:",
    "ts-jest": "^29.1.1",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
