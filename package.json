{
  "name": "@pythnetwork/pyth-crosschain",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.7.0"
  },
  "scripts": {
    "install:modules": "[ $CI ] && true || pnpm install",
    "turbo": "[ ! -d node_modules ] && pnpm install; turbo",
    "fix:lint": "biome check --write",
    "fix:lint:unsafe": "biome check --write --unsafe",
    "test:lint": "biome check"
  },
  "devDependencies": {
    "@better-builds/ts-duality": "catalog:",
    "@biomejs/biome": "catalog:",
    "@pythnetwork/create-pyth-package": "workspace:",
    "@cprussin/tsconfig": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  }
}