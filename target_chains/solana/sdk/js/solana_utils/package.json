{
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/web3.js": "^1.90.0",
    "bs58": "^5.0.0",
    "jito-ts": "^3.0.1",
    "ts-log": "^2.2.7"
  },
  "description": "Utility functions for Solana",
  "devDependencies": {
    "@pythnetwork/jest-config": "workspace:",
    "@solana/wallet-adapter-react": "^0.15.28",
    "@types/jest": "^29.4.0",
    "@types/node": "^22.5.1",
    "jest": "^29.4.0",
    "prettier": "catalog:",
    "quicktype": "^23.0.76",
    "ts-jest": "^29.0.5"
  },
  "engines": {
    "node": "^24.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/esm/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    },
    "./jito": {
      "import": {
        "default": "./dist/esm/jito.mjs",
        "types": "./dist/esm/jito.d.ts"
      },
      "require": {
        "default": "./dist/cjs/jito.cjs",
        "types": "./dist/cjs/jito.d.ts"
      }
    },
    "./package.json": "./package.json",
    "./transaction": {
      "import": {
        "default": "./dist/esm/transaction.mjs",
        "types": "./dist/esm/transaction.d.ts"
      },
      "require": {
        "default": "./dist/cjs/transaction.cjs",
        "types": "./dist/cjs/transaction.d.ts"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://pyth.network",
  "keywords": [
    "pyth",
    "oracle"
  ],
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "name": "@pythnetwork/solana-utils",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "target_chains/solana/sdk/js/solana_utils",
    "type": "git",
    "url": "https://github.com/pyth-network/pyth-crosschain.git"
  },
  "scripts": {
    "build": "ts-duality",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "pnpm run build && pnpm test:unit",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "test:unit": "jest",
    "version": "pnpm run format && git add -A src"
  },
  "type": "module",
  "types": "./dist/cjs/index.d.ts",
  "version": "0.6.0"
}
