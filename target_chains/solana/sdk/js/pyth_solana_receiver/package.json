  "keywords": [
    "pyth",
    "oracle"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@pythnetwork/hermes-client": "workspace:*",
    "@types/jest": "^29.4.0",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "jest": "^29.4.0",
    "prettier": "catalog:",
    "quicktype": "^23.0.76",
    "ts-jest": "^29.0.5",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@noble/hashes": "^1.4.0",
    "@pythnetwork/price-service-sdk": "workspace:*",
