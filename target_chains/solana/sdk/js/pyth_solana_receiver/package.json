    "jest": "^29.4.0",
    "prettier": "^2.6.2",
    "quicktype": "^23.0.76",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
