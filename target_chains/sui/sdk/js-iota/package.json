{
  "name": "@pythnetwork/pyth-iota-js",
  "version": "2.1.0",
  "description": "Pyth Network IOTA Utilities",
  "homepage": "https://pyth.network",
  "author": {
    "name": "Pyth Data Association"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.mts",
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pyth-network/pyth-crosschain",
    "directory": "target_chains/sui/sdk/js-iota"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/esm/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "types": "./dist/esm/index.d.ts"
      },
      "./client": {
        "import": "./dist/esm/client.mjs",
        "require": "./dist/cjs/client.cjs",
        "types": "./dist/esm/client.d.ts"
      },
      "./examples/IotaRelay": {
        "import": "./dist/esm/examples/IotaRelay.mjs",
        "require": "./dist/cjs/examples/IotaRelay.cjs",
        "types": "./dist/esm/examples/IotaRelay.d.ts"
      },
      "./IotaPriceServiceConnection": {
        "import": "./dist/esm/IotaPriceServiceConnection.mjs",
        "require": "./dist/cjs/IotaPriceServiceConnection.cjs",
        "types": "./dist/esm/IotaPriceServiceConnection.d.ts"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "build-ts-package",
    "example-relay": "pnpm run build && node lib/examples/SuiRelay.js",
    "test:lint": "eslint src/",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "fix:lint": "eslint --fix src/",
    "fix:format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run build && pnpm test:lint",
    "preversion": "pnpm run test:lint",
    "version": "pnpm run format && git add -A src"
  },
  "keywords": [
    "pyth",
    "oracle",
    "iota"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@truffle/hdwallet-provider": "^2.1.5",
    "@types/ethereum-protocol": "^1.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/web3-provider-engine": "^14.0.1",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.14.0",
    "jest": "^29.4.1",
    "prettier": "catalog:",
    "ts-jest": "^29.0.5",
    "typescript": "^5.3.3",
    "web3": "^1.8.2",
    "yargs": "^17.0.20"
  },
  "dependencies": {
    "@iota/iota-sdk": "^0.5.0",
    "@pythnetwork/price-service-client": "workspace:*",
    "buffer": "^6.0.3"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10.19.0"
  },
  "packageManager": "pnpm@10.19.0",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./examples/IotaRelay": "./src/examples/IotaRelay.ts",
    "./IotaPriceServiceConnection": "./src/IotaPriceServiceConnection.ts",
    "./package.json": "./package.json"
  }
}