{
  "author": {
    "name": "Pyth Data Association"
  },
  "dependencies": {
    "@iota/iota-sdk": "^0.5.0",
    "@pythnetwork/price-service-client": "workspace:*",
    "buffer": "^6.0.3"
  },
  "description": "Pyth Network IOTA Utilities",
  "devDependencies": {
    "@pythnetwork/jest-config": "workspace:",
    "@truffle/hdwallet-provider": "^2.1.5",
    "@types/ethereum-protocol": "^1.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/web3-provider-engine": "^14.0.1",
    "@types/yargs": "^17.0.20",
    "jest": "^29.4.1",
    "prettier": "catalog:",
    "web3": "^1.8.2",
    "yargs": "^17.0.20"
  },
  "engines": {
    "node": "^24.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/esm/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    },
    "./client": {
      "import": {
        "default": "./dist/esm/client.mjs",
        "types": "./dist/esm/client.d.ts"
      },
      "require": {
        "default": "./dist/cjs/client.cjs",
        "types": "./dist/cjs/client.d.ts"
      }
    },
    "./IotaPriceServiceConnection": {
      "import": {
        "default": "./dist/esm/IotaPriceServiceConnection.mjs",
        "types": "./dist/esm/IotaPriceServiceConnection.d.ts"
      },
      "require": {
        "default": "./dist/cjs/IotaPriceServiceConnection.cjs",
        "types": "./dist/cjs/IotaPriceServiceConnection.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://pyth.network",
  "keywords": [
    "pyth",
    "oracle",
    "iota"
  ],
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "name": "@pythnetwork/pyth-iota-js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "target_chains/sui/sdk/js-iota",
    "type": "git",
    "url": "https://github.com/pyth-network/pyth-crosschain"
  },
  "scripts": {
    "build": "ts-duality",
    "clean": "rm -rf ./dist",
    "example-relay": "pnpm run build && node lib/examples/SuiRelay.js",
    "prepublishOnly": "pnpm run build",
    "test:format": "prettier --check \"src/**/*.ts\"",
    "version": "pnpm run format && git add -A src"
  },
  "type": "module",
  "types": "./dist/cjs/index.d.ts",
  "version": "2.2.0"
}
