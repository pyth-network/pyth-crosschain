{
  "name": "@pythnetwork/test-config",
  "description": "baseline configuration file to use when running tests with vitest",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.7.0"
  },
  "packageManager": "pnpm@10.19.0",
  "type": "module",
  "files": [
    "unit-test.js",
    "config.cjs",
    "config.js"
  ],
  "bin": {
    "unit-test": "./unit-test.js"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./config": {
      "require": {
        "default": "./dist/cjs/config.cjs",
        "types": "./dist/cjs/config.d.ts"
      },
      "import": {
        "default": "./dist/esm/config.mjs",
        "types": "./dist/esm/config.d.ts"
      }
    },
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/esm/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "ts-duality",
    "clean": "rm -rf dist/",
    "fix:format": "prettier --write .",
    "fix:lint:eslint": "eslint --fix ."
  },
  "dependencies": {
    "@cprussin/eslint-config": "catalog:",
    "@cprussin/tsconfig": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "happy-dom": "catalog:",
    "prettier": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
