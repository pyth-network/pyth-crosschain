{
  "name": "@pythnetwork/test-config",
  "description": "baseline configuration file to use when running tests with vitest",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.7.0"
  },
  "packageManager": "pnpm@10.19.0",
  "type": "module",
  "files": [
    "unit-test.js",
    "config.cjs",
    "config.js"
  ],
  "bin": {
    "unit-test": "./unit-test.js"
  },
  "main": "./config.cjs",
  "module": "./config.js",
  "types": "./config.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./config.js",
        "types": "./config.d.ts"
      },
      "require": {
        "default": "./config.cjs",
        "types": "./config.d.ts"
      }
    }
  },
  "scripts": {
    "fix:format": "prettier --write .",
    "fix:lint:eslint": "eslint --fix ."
  },
  "dependencies": {
    "@cprussin/eslint-config": "catalog:",
    "@cprussin/tsconfig": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "happy-dom": "catalog:",
    "prettier": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
