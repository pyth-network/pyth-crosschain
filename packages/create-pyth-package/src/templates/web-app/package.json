  },
  "dependencies": {
    "@next/third-parties": "catalog:",
    "@pythnetwork/component-library": "workspace:*",
    "@pythnetwork/react-hooks": "workspace:*",
    "@pythnetwork/shared-lib": "workspace:",
    "@react-hookz/web": "catalog:",
    "clsx": "catalog:",
    "framer-motion": "catalog:",
    "next": "catalog:",
    "nuqs": "catalog:",
    "pino": "catalog:",
    "react": "catalog:",
