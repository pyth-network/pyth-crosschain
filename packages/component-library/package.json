{
  "name": "@pythnetwork/component-library",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20"
  },
  "exports": {
    "./*": "./src/*/index.tsx",
    "./theme": "./src/theme.scss"
  },
  "scripts": {
    "build:storybook": "storybook build",
    "fix:format": "prettier --write .",
    "fix:lint:eslint": "eslint --fix .",
    "fix:lint:stylelint": "stylelint --fix 'src/**/*.scss'",
    "start:dev": "storybook dev --port 4000 --no-open",
    "test:format": "prettier --check .",
    "test:lint:eslint": "eslint .",
    "test:lint:stylelint": "stylelint 'src/**/*.scss'",
    "test:types": "tsc"
  },
  "peerDependencies": {
    "next": "catalog:",
    "react": "catalog:"
  },
  "dependencies": {
    "@pythnetwork/fonts": "workspace:*",
    "@react-hookz/web": "catalog:",
    "clsx": "catalog:",
    "modern-normalize": "catalog:",
    "motion": "catalog:",
    "react-aria": "catalog:",
    "react-aria-components": "catalog:"
  },
  "devDependencies": {
    "@cprussin/eslint-config": "catalog:",
    "@cprussin/jest-config": "catalog:",
    "@cprussin/prettier-config": "catalog:",
    "@cprussin/tsconfig": "catalog:",
    "@phosphor-icons/react": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-styling-webpack": "catalog:",
    "@storybook/addon-themes": "catalog:",
    "@storybook/blocks": "catalog:",
    "@storybook/nextjs": "catalog:",
    "@storybook/react": "catalog:",
    "@types/jest": "catalog:",
    "@types/react": "catalog:",
    "autoprefixer": "catalog:",
    "css-loader": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "next": "catalog:",
    "postcss": "catalog:",
    "postcss-loader": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "sass": "catalog:",
    "sass-loader": "catalog:",
    "storybook": "catalog:",
    "style-loader": "catalog:",
    "stylelint": "catalog:",
    "stylelint-config-standard-scss": "catalog:",
    "typescript": "catalog:"
  }
