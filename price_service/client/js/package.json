  "keywords": [
    "pyth",
    "oracle"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "jest": "^29.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.6.3",
    "yargs": "^17.4.1"
  },
  "dependencies": {
    "@pythnetwork/price-service-sdk": "*",
    "@types/ws": "^8.5.3",
    "axios": "^1.2.5",
    "axios-retry": "^3.4.0",
