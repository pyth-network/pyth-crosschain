{
  "dependencies": {
    "bn.js": "^5.2.1"
  },
  "description": "Pyth price service SDK",
  "devDependencies": {
    "@cprussin/tsconfig": "catalog:",
    "@pythnetwork/jest-config": "workspace:",
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.0",
    "quicktype": "^23.0.76",
    "typescript": "catalog:"
  },
  "engines": {
    "node": "^24.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/esm/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/cjs/index.d.ts"
      }
    },
    "./AccumulatorUpdateData": {
      "import": {
        "default": "./dist/esm/AccumulatorUpdateData.mjs",
        "types": "./dist/esm/AccumulatorUpdateData.d.ts"
      },
      "require": {
        "default": "./dist/cjs/AccumulatorUpdateData.cjs",
        "types": "./dist/cjs/AccumulatorUpdateData.d.ts"
      }
    },
    "./package.json": "./package.json",
    "./schemas/PriceFeed": {
      "import": {
        "default": "./dist/esm/schemas/PriceFeed.mjs",
        "types": "./dist/esm/schemas/PriceFeed.d.ts"
      },
      "require": {
        "default": "./dist/cjs/schemas/PriceFeed.cjs",
        "types": "./dist/cjs/schemas/PriceFeed.d.ts"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://pyth.network",
  "keywords": [
    "pyth",
    "oracle"
  ],
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "name": "@pythnetwork/price-service-sdk",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "price_service/sdk/js",
    "type": "git",
    "url": "https://github.com/pyth-network/pyth-crosschain.git"
  },
  "scripts": {
    "build": "ts-duality",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "pnpm run build && pnpm test:unit",
    "test:unit": "jest",
    "version": "pnpm run format && git add -A src"
  },
  "type": "module",
  "types": "./dist/cjs/index.d.ts",
  "version": "1.9.0"
}
