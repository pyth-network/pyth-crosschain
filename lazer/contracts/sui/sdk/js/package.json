{
  "dependencies": {
    "@mysten/sui": "^2.5.0"
  },
  "description": "TypeScript SDK for the Pyth Lazer Sui contract",
  "devDependencies": {
    "@cprussin/tsconfig": "catalog:",
    "@pythnetwork/jest-config": "workspace:",
    "@pythnetwork/pyth-lazer-sdk": "^5.2.0",
    "@types/jest": "catalog:",
