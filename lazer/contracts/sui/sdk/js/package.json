{
  "name": "@pythnetwork/pyth-lazer-sui-js",
  "version": "0.1.0",
  "description": "TypeScript SDK to compose Sui PTBs for Pyth Lazer Sui contract",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "fix": "pnpm prettier --write src && pnpm eslint --fix src --ext .ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@mysten/sui": "^1.3.0",
    "@pythnetwork/pyth-lazer-sdk": "workspace:*",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@cprussin/eslint-config": "catalog:",
    "@cprussin/tsconfig": "catalog:",
    "@types/node": "^18.11.18",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
